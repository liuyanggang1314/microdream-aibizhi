/* pages/me/me.wxss */
.me-head {
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 35vh;
  line-height: 35vh;
  background: linear-gradient(to right, #7BC6CC, #BE93C5);
}

.avatar {
  height: 5rem;
}

.userinfo-nickname {
  color: #ffffff;
}

.me-foot {
  text-align: center;
  position: absolute;
  bottom: 8px;
  left: 0px;
  right: 0px;
}

.di {
  color: #FC5C7D;
  font-size: 11px;
}